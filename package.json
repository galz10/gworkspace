{
  "name": "gworkspace",
  "version": "0.1.0",
  "description": "Bun TypeScript CLI for Google Workspace context",
  "type": "module",
  "bin": {
    "gworkspace": "dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --target=node --outdir dist --minify",
    "dev": "bun run src/index.ts",
    "start": "bun run src/index.ts",
    "check": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "@types/bun": "^1.3.0"
  }
}
